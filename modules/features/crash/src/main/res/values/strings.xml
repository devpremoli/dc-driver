    <string name="crash_assist_tutorial_tour_step_two_body">CrashAssist pinpoints your location - and we’ll send help if needed.</string>

    <string name="crash_assist_tutorial_tour_step_three_title">Driver Notified</string>
    <string name="crash_assist_tutorial_tour_step_three_body">CrashAssist will notify you. If we don’t hear from you, we’ll call you.</string>

    <string name="crash_assist_tutorial_tour_step_four_title">Driver Called</string>
    <string name="crash_assist_tutorial_tour_step_four_body">CrashAssist calls you 3 times. If we don’t hear from you, we’ll send help.</string>

    <string name="crash_assist_tutorial_tour_step_five_title">Add us to your contacts</string>
    <string name="crash_assist_tutorial_tour_step_five_body">Easily recognize CrashAssist when we call to get you help in a crash.</string>
