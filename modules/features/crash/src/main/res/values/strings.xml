    <string name="crash_assist_tutorial_tour_start_title">Introducing CrashAssist</string>
    <string name="crash_assist_tutorial_tour_start_body">Get help instantly in a crash with automatic crash detection and 24/7 emergency response.</string>
    <string name="crash_assist_tutorial_tour_button">Next</string>

    <string name="crash_assist_tutorial_tour_step_one_title">Crash Detected</string>
